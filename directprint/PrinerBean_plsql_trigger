SET_CUSTOM_PROPERTY('PRINTER_BLOCK.PRINTER_BEAN',1,'LOAD_PRINTERS','X');


-----

DECLARE
   v_list VARCHAR2(4000);
BEGIN
   v_list :=
     GET_CUSTOM_PROPERTY('PRINTER_BLOCK.PRINTER_BEAN',1,'GET_PRINTERS');

   MESSAGE(v_list);
END;


----

SET_CUSTOM_PROPERTY('PRINTER_BLOCK.PRINTER_BEAN',1,
                    'PRINTER_NAME',
                    :PRINTER_BLOCK.PRINTER_NAME);

:PRINTER_BLOCK.PRINTER_TYPE :=
   GET_CUSTOM_PROPERTY('PRINTER_BLOCK.PRINTER_BEAN',1,'AUTO_DETECT');

